s="ABCD"
def com(s,total,count,i,j):
    if total == count:
        return
    else:
        if j == len(s):
            i = i +1
            j = i + 1
        print(s[i],s[j])
        count+=1
        com(s,total,count,i,j+1)
total = (len(s) * (len(s)-1))
count = 0
i = 0
j = 1
com(s,total,count,i,j)